{
  "name": "aeonian",
  "version": "0.0.1",
  "description": "Aeonian Home Automation",
  "main": "./bin/server.js",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "lint": "tslint './aeonian/**/*.ts'",
    "lint:fix": "tslint --fix './aeonian/**/*.ts'",
    "lint:both": "npm run lint:fix && npm run lint",
    "prebuild": "npm run clean && npm run lint:both",
    "build": "tsc",
    "dev": "nodemon -e ts -w aeonian -x 'npm run build && npm start'",
    "start": "node bin/server.js",
    "test": "npm run lint",
    "ui": "npm run webpack -- --config webpack.config.js --progress --profile",
    "webpack": "node node_modules/webpack/bin/webpack.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rcodonnell/Aeonian.git"
  },
  "author": "Corey O'Donnell <rcodonnell>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rcodonnell/Aeonian/issues"
  },
  "homepage": "https://github.com/rcodonnell/Aeonian#readme",
  "dependencies": {
    "@angular/common": "^4.2.3",
    "@angular/compiler": "^4.2.3",
    "@angular/core": "^4.2.3",
    "@angular/forms": "^4.2.3",
    "@angular/http": "^4.2.3",
    "@angular/platform-browser": "^4.2.3",
    "@angular/platform-browser-dynamic": "^4.2.3",
    "@angular/router": "^4.2.3",
    "@types/body-parser": "^1.16.3",
    "@types/console-stamp": "^0.2.30",
    "@types/express": "^4.0.35",
    "@types/lodash": "^4.14.65",
    "@types/multer": "0.0.34",
    "body-parser": "^1.17.2",
    "console-stamp": "^0.2.5",
    "core-js": "^2.4.1",
    "express": "^4.15.3",
    "lodash": "^4.17.4",
    "multer": "^1.3.0",
    "reflect-metadata": "^0.1.10",
    "routing-controllers": "^0.7.0",
    "rxjs": "^5.4.1",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.3",
    "css-loader": "^0.28.4",
    "html-webpack-plugin": "^2.28.0",
    "nodemon": "^1.11.0",
    "raw-loader": "^0.5.1",
    "script-ext-html-webpack-plugin": "^1.8.1",
    "to-string-loader": "^1.1.5",
    "tslint": "^5.4.3",
    "typescript": "^2.3.4",
    "webpack": "^3.0.0"
  }
}
