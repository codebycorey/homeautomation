{
  "name": "aeonian",
  "version": "0.0.1",
  "description": "Aeonian Home Automation",
  "main": "./bin/server.js",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "start": "node bin/server.js",
    "tsc": "tsc",
    "build": "npm run clean && npm run lint && npm run tsc",
    "watch": "nodemon -e ts -w aeonian -x 'npm run build'",
    "prelint": "tslint --fix './aeonian/**/*.ts'",
    "lint": "tslint './aeonian/**/*.ts'",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rcodonnell/Aeonian.git"
  },
  "author": "Corey O'Donnell <rcodonnell>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rcodonnell/Aeonian/issues"
  },
  "homepage": "https://github.com/rcodonnell/Aeonian#readme",
  "dependencies": {
    "@types/body-parser": "^1.16.3",
    "@types/express": "^4.0.35",
    "@types/lodash": "^4.14.65",
    "@types/multer": "0.0.34",
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "lodash": "^4.17.4",
    "multer": "^1.3.0",
    "reflect-metadata": "^0.1.10",
    "routing-controllers": "^0.7.0"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "tslint": "^5.4.3",
    "typescript": "^2.3.4"
  }
}
